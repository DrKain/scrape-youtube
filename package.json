  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/request": "^2.48.4",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
